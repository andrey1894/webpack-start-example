{
  "name": "Webpack start example",
  "version": "1.0.0",
  "description": "Webpack start example",
  "author": "Andrey",
  "scripts": {
    "start": "yarn build:watch",
    "build": "webpack --config webpack.config.local.js",
    "build:watch": "webpack --watch --progress --config webpack.config.local.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build:dev:watch": "webpack --watch --progress --config webpack.config.dev.js",
    "build:prod": "webpack --config webpack.config.prod.js",
    "build:prod:watch": "webpack --watch --progress --config webpack.config.prod.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "babel-plugin-import-directory": "^1.1.0"
  },
  "devDependencies": {
    "lodash": "^4.17.21",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "expose-loader": "^3.1.0",
    "file-system": "^2.2.2",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "import-glob-loader": "^1.1.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass-glob-importer": "^5.3.2",
    "path": "^0.12.7",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.5.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.0.1",
    "stylelint-webpack-plugin": "^3.1.0",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrey1894/webpack-start-example"
  }
}
